# Production Environment
NODE_ENV=production
REACT_APP_API_URL=https://api.ordnungshub.com
REACT_APP_FALLBACK_API_URL=https://backup.ordnungshub.com
REACT_APP_API_TIMEOUT=15000
REACT_APP_ENVIRONMENT=production

# Security
REACT_APP_ENABLE_DEBUG=false
REACT_APP_LOG_LEVEL=error

# Database (Backend only)
DB_PASSWORD=secure_production_password
SECRET_KEY=production_secret_key_64_chars_long_secure_random
JWT_SECRET_KEY=production_jwt_secret_key_64_chars_long_secure

# CORS (Production domains)
CORS_ORIGINS=https://ordnungshub.com,https://www.ordnungshub.com,https://app.ordnungshub.com
